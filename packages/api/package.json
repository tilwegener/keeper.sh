{
  "name": "@keeper.sh/api",
  "main": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --hot src/index.ts | pino-pretty",
    "build": "bun build src/index.ts src/routes/**/*.ts --outdir dist --target bun --splitting",
    "start": "bun run dist/index.js",
    "types": "tsc --noEmit"
  },
  "devDependencies": {
    "@keeper.sh/typescript-config": "workspace:*",
    "@types/bun": "latest",
    "pino-pretty": "13.1.3",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@keeper.sh/auth": "workspace:*",
    "@keeper.sh/broadcast": "workspace:*",
    "@keeper.sh/calendar": "workspace:*",
    "@keeper.sh/data-schemas": "workspace:*",
    "@keeper.sh/database": "workspace:*",
    "@keeper.sh/destination-providers": "workspace:*",
    "@keeper.sh/encryption": "workspace:*",
    "@keeper.sh/env": "workspace:*",
    "@keeper.sh/integration-caldav": "workspace:*",
    "@keeper.sh/integrations": "workspace:*",
    "@keeper.sh/log": "workspace:*",
    "@keeper.sh/oauth-google": "workspace:*",
    "@keeper.sh/premium": "workspace:*",
    "drizzle-orm": "0.45.1",
    "ts-ics": "2.4.0"
  }
}
