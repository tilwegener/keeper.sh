  },
  "dependencies": {
    "@keeper.sh/auth": "workspace:*",
    "@keeper.sh/calendar": "workspace:*",
    "@keeper.sh/database": "workspace:*",
    "@keeper.sh/integration-google-calendar": "workspace:*",
    "@keeper.sh/integrations": "workspace:*",
    "@keeper.sh/log": "workspace:*",
    "@keeper.sh/premium": "workspace:*",
    "cronbake": "0.4.0",
    "drizzle-orm": "^0.45.1"
  }
}
